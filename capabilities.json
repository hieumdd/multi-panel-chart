{
    "dataRoles": [
        {
            "displayName": "Date",
            "name": "group",
            "kind": "Grouping"
        },
        {
            "displayName": "Measures",
            "name": "measures",
            "kind": "Measure",
            "requiredTypes": [
                {
                    "numeric": true
                }
            ]
        }
    ],
    "drilldown": {
        "roles": ["group"]
    },
    "objects": {
        "legend": {
            "displayName": "Legend",
            "properties": {
                "fontSize": {
                    "displayName": "Font Size",
                    "type": {
                        "numeric": true
                    }
                }
            }
        },
        "axis": {
            "displayName": "Axis",
            "properties": {
                "fontSize": {
                    "displayName": "Font Size",
                    "type": {
                        "numeric": true
                    }
                }
            }
        },
        "tooltip": {
            "displayName": "Tooltip",
            "properties": {
                "fontSize": {
                    "displayName": "Font Size",
                    "type": {
                        "numeric": true
                    }
                },
                "panelGap": {
                    "displayName": "Panel Gap (em)",
                    "type": {
                        "numeric": true
                    }
                }
            }
        },
        "dataPoint": {
            "displayName": "Data Point",
            "properties": {
                "dataPoint": {
                    "displayName": "Data Point Dot",
                    "type": {
                        "bool": true
                    }
                }
            }
        },
        "panel": {
            "displayName": "Panels",
            "properties": {
                "panel": {
                    "type": {
                        "enumeration": [
                            { "displayName": "1", "value": "1" },
                            { "displayName": "2", "value": "2" },
                            { "displayName": "3", "value": "3" },
                            { "displayName": "4", "value": "4" },
                            { "displayName": "5", "value": "5" }
                        ]
                    }
                }
            }
        },
        "yAxis": {
            "displayName": "Y Axis",
            "properties": {
                "align": {
                    "type": {
                        "enumeration": [
                            { "displayName": "Left", "value": "left" },
                            { "displayName": "Right", "value": "right" }
                        ]
                    }
                },
                "inverse": {
                    "type": {
                        "bool": true
                    }
                },
                "mmOverride": {
                    "type": {
                        "bool": true
                    }
                },
                "mmMin": {
                    "type": {
                        "numeric": true
                    }
                },
                "mmMax": {
                    "type": {
                        "numeric": true
                    }
                }
            }
        },
        "series": {
            "displayName": "Series",
            "properties": {
                "color": {
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "area": {
                    "type": {
                        "bool": true
                    }
                }
            }
        }
    },
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "date": {
                        "max": 1
                    },
                    "measures": {}
                }
            ],
            "table": {
                "rows": {
                    "select": [
                        {
                            "bind": {
                                "to": "date"
                            }
                        },
                        {
                            "for": {
                                "in": "measures"
                            }
                        }
                    ]
                }
            },
            "categorical": {
                "categories": {
                    "for": {
                        "in": "group"
                    }
                },
                "values": {
                    "select": [
                        {
                            "for": {
                                "in": "measures"
                            }
                        }
                    ]
                }
            }
        }
    ]
}
